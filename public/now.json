{
  "version": 2,
  "alias": "foodrecipes",
  "name": "foodrecipes",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    },
    { 
      "src": "^/manifest.json$", 
      "dest": "/manifest.json" 
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    }
  ]
}


